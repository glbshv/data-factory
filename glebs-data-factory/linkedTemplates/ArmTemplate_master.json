{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "glebs-data-factory"
		},
		"Real estate blob_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Real estate blob'"
		},
		"glebs database on azure_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'glebs database on azure'"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "glebs-data-factory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"Real estate blob_connectionString": {
						"value": "[parameters('Real estate blob_connectionString')]"
					},
					"glebs database on azure_connectionString": {
						"value": "[parameters('glebs database on azure_connectionString')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}